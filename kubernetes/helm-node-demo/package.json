  "main": "src/index.mjs",
  "scripts": {
    "start": "node src/index.mjs",
    "docker-build": "docker build --debug --progress=plain -f Dockerfile -t node-server:0.0.1 .",
    "docker-run": "docker run -p 7001:7001 node-server:0.0.1",
